MDBoxLayout:
    orientation: 'vertical'
    padding: ('0dp', '20dp', '10dp', '20dp')
    spacing: '10dp'

    MDCard:
        pos_hint: {'center_x': 0.5,'center_y': 0.5}
        size_hint: .8, None
        md_bg_color: [0,1,0,1]
        MDIcon:
            icon: 'phone'
            pos_hint: {'top': 1,'right': 1}